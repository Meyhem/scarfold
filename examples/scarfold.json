{
  "templateFolder": "templates",
  "scaffolding": {
    "component": {
      "render": [
        {
          "template": "react.component.hbs",
          "dest": "src/components/{{name}}.ts"
        }
      ],
      "vars": {
        "name": { },
        "withRedux": { 
          "default": false
        }
      }
    }
  }
}