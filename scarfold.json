{
  "templateFolder": "templates",
  "graceful": false,
  "scaffolding": {
    "textfile": {
      "render": [
        {
          "template": "textfile.txt",
          "dest": "test/src/##name##.txt"
        },
        {
          "template": "textfile2.txt",
          "dest": "test/src/##name##2.txt"
        }
      ],
      "vars": {
        "name": { },
        "status": {
          "default": "simple"
        }
      }
    }
  }
}